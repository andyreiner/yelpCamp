{
    "version": 2,
    "name": "nodejs-express",
    "builds": [
        {
            "src": "app.js",
            "use": "@now/node"
        }  
    ],
    "redirects": [
        { "src": "/routes", "dest": "/(*)" },
        { "src": "/views", "dest": "/(*)" }
    ],
    "functions": ["/cloudinary/(*)", "/controllers/(*)", "/scripts/(*)", "/utils(*)"]
        
}