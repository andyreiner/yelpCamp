{
    "version": 2,
    "name": "nodejs-express",
    "builds": [
        {
            "src": "app.js",
            "use": "@now/node"
        }  
    ],
    "routes": [
        { "src": "/routes", "dest": "app.js" }
    ],
    "functions": {
    "cloudinary/*": {
      "memory": 3008,
      "maxDuration": 30
    },
    "controllers/*.js": {
      "memory": 3008,
      "maxDuration": 30
    },
    "utils/*.js": {
      "memory": 3008,
      "maxDuration": 30
    },
    "scripts/*.js": {
      "memory": 3008,
      "maxDuration": 30
    },
    "middleware.js": {
      "memory": 3008,
      "maxDuration": 30
    },
    "schemas.js": {
      "memory": 3008,
      "maxDuration": 30
    }
  }
}